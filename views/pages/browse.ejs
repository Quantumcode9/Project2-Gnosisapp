<%- include('../partials/header') %>
<main>
  <div class="row">
  <h1>TV Show Genres</h1>
  <div class="col-sm-12">
    <% genres.forEach(genre => { %>
        <a href="/pages/genre/<%= genre.id %>">
          <div class="genre-card">
              <h2 class="genre-title"><%= genre.name %></h2>
            </div>
            </a>
        
      <% }); %>
  </div>
  </div>
  </main>
  <%- include('../partials/footer') %>